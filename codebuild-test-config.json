{
    "name": "bia-test",
    "serviceRole": "arn:aws:iam::397234361193:role/service-role/bia-test",
    "description": "Projeto CodeBuild para execução de testes",
    "environment": {
        "type": "LINUX_CONTAINER",
        "image": "aws/codebuild/amazonlinux-x86_64-standard:5.0",
        "computeType": "BUILD_GENERAL1_SMALL",
        "environmentVariables": [
            {
                "name": "NODE_ENV",
                "value": "test",
                "type": "PLAINTEXT"
            }
        ],
        "privilegedMode": false,
        "imagePullCredentialsType": "CODEBUILD"
    },
    "source": {
        "type": "CODEPIPELINE",
        "buildspec": "pipeline/buildspec-test.yml",
        "insecureSsl": false
    },
    "artifacts": {
        "type": "CODEPIPELINE"
    },
    "timeoutInMinutes": 15,
    "queuedTimeoutInMinutes": 480
}